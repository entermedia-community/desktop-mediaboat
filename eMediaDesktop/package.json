{
  "name": "emedia-library",
  "productName": "eMedia Library",
  "version": "2.2.8",
  "description": "eMedia Library",
  "main": "src/index.js",
  "homepage": "https://github.com/entermedia-community/desktop-mediaboat",
  "scripts": {
    "start": "NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build": "electron-builder build",
    "publish": "electron-forge publish",
    "distwin": "electron-builder --win zip",
    "distlinux": "electron-builder --linux deb tar.xz ",
    "distmacarm64": "NODE_ENV=production electron-builder --arm64 --mac dmg",
    "distmacx64": "NODE_ENV=production electron-builder --x64 --mac dmg"
  },
  "keywords": [
    "DAM",
    "Workspace",
    "EnterMediaDB",
    "Media Library"
  ],
  "author": {
    "name": "EnterMedia",
    "email": "info@entermediadb.org"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "chokidar": "^3.6.0",
    "electron-log": "5.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "ext-name": "^5.0.0",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "mkdirp": "^3.0.1",
    "querystring": "^0.2.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "unused-filename": "^2.1.0",
    "url": "^0.11.3",
    "wine": "^0.9.13"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "electron": "28.1.1",
    "electron-builder": "^24.9.1",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-installer-windows": "^3.0.0",
    "electron-reloader": "^1.2.3"
  },
  "build": {
    "appId": "org.entermedia.app",
    "productName": "eMedia Library",
    "asar": false,
    "icon": "build/icons",
    "files": [
      "src/**/*"
    ],
    "protocols": [
      {
        "name": "entermedia",
        "role": "Viewer",
        "schemes": [
          "entermedia"
        ]
      }
    ],
    "linux": {
      "icon": "build/icons",
      "category": "AudioVideo"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons"
    },
    "nsis": {
      "perMachine": true
    },
    "mac": {
      "icon": "build/icons",
      "identity": "tech@entermediadb.org"
    }
  }
}
