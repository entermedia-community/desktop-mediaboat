{
  "name": "emedia-library",
  "productName": "eMedia Library",
  "version": "2.3.5",
  "description": "Digital Asset Management Solution",
  "main": "src/index.js",
  "homepage": "https://github.com/entermedia-community/desktop-mediaboat",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build": "electron-builder build",
    "publish": "electron-forge publish",
    "dmg": "electron-forge make --arch=x64 --platform=darwin && electron-forge make --arch=arm64 --platform=darwin",
    "pkg": "electron-forge make --arch=universal --platform=mas",
    "linux": "electron-forge make --arch=x64 --platform=linux",
    "zip": "electron-forge make --arch=x64"
  },
  "keywords": [
    "DAM",
    "Workspace",
    "EnterMediaDB",
    "Media Library"
  ],
  "author": {
    "name": "eMedia Library",
    "email": "info@entermediadb.org"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "chokidar": "^3.6.0",
    "dotenv": "^16.4.5",
    "electron-dl": "^3.5.2",
    "electron-log": "5.0.3",
    "electron-store": "^8.1.0",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "unused-filename": "^2.1.0",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.5.0",
    "@electron-forge/maker-pkg": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/publisher-github": "^7.5.0",
    "@electron/notarize": "^2.5.0",
    "@electron/osx-sign": "^1.3.1",
    "@electron/universal": "^2.0.1",
    "electron": "28.1.1",
    "electron-builder": "^24.9.1",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-installer-windows": "^3.0.0",
    "electron-reloader": "^1.2.3"
  },
  "build": {
    "appId": "com.emedialibrary",
    "productName": "eMedia Library",
    "asar": false,
    "icon": "build/icons",
    "files": [
      "src/**/*"
    ],
    "protocols": [
      {
        "name": "entermedia",
        "role": "Viewer",
        "schemes": [
          "entermedia"
        ]
      }
    ],
    "linux": {
      "icon": "build/icons",
      "category": "AudioVideo"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons"
    },
    "nsis": {
      "perMachine": true
    },
    "mac": {
      "icon": "build/icons",
      "identity": "tech@entermediadb.org"
    }
  }
}
