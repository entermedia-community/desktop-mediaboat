{
  "name": "em-app",
  "productName": "em-app",
  "version": "1.0.0",
  "description": "Entermedia that connects directly to",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "makewin": "electron-forge make --platform win32",
    "makemac": "electron-forge make --platform darwin",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "winbuild": "electron-packager . entermediaapp --platform win32 --overwrite --arch x64 --out dist/",
    "winsetup": "electron-installer-windows --src dist/entermediaapp-win32-x64/ --dest dist/installers/"
  },
  "keywords": [],
  "author": {
    "name": "Mitsuo Yazawa",
    "email": "mitsuo@entermediadb.org"
  },
  "license": "---",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "em_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "options": {
              "maintainer": "EnterMedia <help@entermediadb.org>",
              "homepage": "http://entermediadb.org"
            }
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "EnterMedia <help@entermediadb.org>",
              "homepage": "http://entermediadb.org"
            }
          }
        }
      ]
    }
  },
  "dependencies": {
    "chokidar": "^3.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-appx": "^6.0.0-beta.51",
    "@electron-forge/maker-deb": "^6.0.0-beta.51",
    "@electron-forge/maker-dmg": "^6.0.0-beta.51",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
    "@electron-forge/maker-wix": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "electron": "9.0.5",
    "electron-builder": "^22.7.0",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "*",
    "electron-prebuilt": "*"
  }
}
